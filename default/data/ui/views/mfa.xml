        <title>Uses time picker above</title>
        <search>
          <query>`okta_base_index` eventType="user.authentication.auth_via_mfa" | rename target{}.displayName as targetname |eval authenticator=mvindex(targetname, 1) | search authenticator!="Password" | geostats count latfield=client.geographicalContext.geolocation.lat longfield=client.geographicalContext.geolocation.lon by authenticator</query>
          <earliest>$field1.earliest$</earliest>
          <latest>$field1.latest$</latest>
